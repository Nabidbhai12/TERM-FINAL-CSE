
// C program to demonstrate working of memset()
#include <stdio.h>
#include <string.h>
int substring(char *mstr,char *sstr)
{
    int len1=strlen(mstr);
    int len2=strlen(sstr);
    for(int i=0;i<=len1-len2;i++)
    {
        int j;
        for(j=0;j<len2;j++)
        {
            if(mstr[i+j]!=sstr[j])
                break;
        }
        if(j==len2)
            return i;
    }
    return 0;
}
int main()
{
    char str[50];
    scanf("%s",str);
    char str1[1000];
    scanf("%s",str1);


}
